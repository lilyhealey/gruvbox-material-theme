{
  "tokenColours": [
    {
      "settings": {
        "foreground": "{{variant.colours.white.1}}"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment punctuation",
        "string.quoted.docstring",
        "string.quoted.docstring punctuation"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.white.5}}"
      }
    },
    {
      "name": "Constants - Values",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "Constants - Language",
      "scope": [
        "constant.other",
        "constant.other.php"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "{{variant.colours.medium.blue}}"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.operator",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.blue}}"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.green}}"
      }
    },
    {
      "name": "Support Function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.teal}}"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.medium.blue}}"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "{{variant.colours.medium.blue}}"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": [
        "source.cpp meta.block variable.other"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Variables constant",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.yellow}}"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "variable.parameter.function.language.special",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.teal}}"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.blue}}"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.blue}}"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "{{variant.colours.medium.blue}}"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.green}}"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.red}}"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.purple}}"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "{{variant.colours.light.teal}}"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.medium.blue}}"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "{{variant.colours.light.red}}"
      }
    },
